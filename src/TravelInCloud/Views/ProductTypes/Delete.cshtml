@model TravelInCloud.Models.ProductType

@{
    ViewData["Title"] = "Delete";
}

<form asp-action="Delete">
    <div class="form-actions no-color">

        <div class="page__bd page__bd_spacing">
            <h3>删除商品类型</h3>

            <p>你确定要删除 @Model.ProductTypeName 吗？</p>
        </div>

        <div class="page__bd page__bd_spacing">
            <input type="submit" class="weui-btn weui-btn_warn" value="确认删除">
        </div>

        <div class="weui-btn-area">
            <a class="weui-btn weui-btn_primary" asp-controller="ProductTypes" asp-action="Index" asp-route-ProductId="@Model.BelongingProductId">返回</a>
        </div>
    </div>
</form>

