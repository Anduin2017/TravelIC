                <a class="weui-cell weui-cell_access" href="javascript:;">
                    <div class="weui-cell__hd"><img style="width:20px;margin-right:5px;display:block" alt="" src="data:image/png;base64,"></div>
                    <div class="weui-cell__bd weui-cell_primary">
                        <p>文字标题</p>
                    </div>
                    <span class="weui-cell__ft"></span>
                </a>
